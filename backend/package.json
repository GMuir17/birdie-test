{
  "name": "backend",
  "version": "0.0.0",
  "description": "backend",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "serverless offline start --httpPort 8000 --stage develop",
    "deploy:production": "serverless deploy --stage production  --aws-profile gary-personal",
    "remove:production": "serverless remove --stage production  --aws-profile gary-personal",
    "print": "serverless print --stage production"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1094.0",
    "axios": "^0.21.1",
    "dayjs": "^1.11.7",
    "esbuild": "^0.14.27",
    "mysql2": "^3.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "8.29.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.8.3",
    "puppeteer": "^17.1.3",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^3.12.2",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.5.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
